{"ast":null,"code":"import sweetalert from 'sweetalert';\nimport Todo from './Todo';\nexport default {\n  props: ['todos'],\n  components: {\n    Todo\n  },\n  methods: {\n    deleteTodo(todo) {\n      sweetalert({\n        title: 'Are you sure?',\n        text: 'This To-Do will be permanently deleted!',\n        type: 'warning',\n        showCancelButton: true,\n        confirmButtonColor: '#DD6B55',\n        confirmButtonText: 'Yes, delete it!',\n        closeOnConfirm: false\n      }, () => {\n        const todoIndex = this.todos.indexOf(todo);\n        this.todos.splice(todoIndex, 1);\n        sweetalert('Deleted!', 'Your To-Do has been deleted.', 'success');\n      });\n    },\n\n    completeTodo(todo) {\n      const todoIndex = this.todos.indexOf(todo);\n      this.todos[todoIndex].done = true;\n      sweetalert('Success!', 'To-Do completed!', 'success');\n    }\n\n  }\n};","map":{"version":3,"mappings":"AASA,OAAOA,UAAP,MAAuB,YAAvB;AACA,OAAOC,IAAP,MAAiB,QAAjB;AAEA,eAAe;EACbC,KAAK,EAAE,CAAC,OAAD,CADM;EAEbC,UAAU,EAAE;IACVF;EADU,CAFC;EAKbG,OAAO,EAAE;IACPC,UAAU,CAACC,IAAD,EAAO;MACfN,UAAU,CAAC;QACTO,KAAK,EAAE,eADE;QAETC,IAAI,EAAE,yCAFG;QAGTC,IAAI,EAAE,SAHG;QAITC,gBAAgB,EAAE,IAJT;QAKTC,kBAAkB,EAAE,SALX;QAMTC,iBAAiB,EAAE,iBANV;QAOTC,cAAc,EAAE;MAPP,CAAD,EASV,MAAM;QACJ,MAAMC,SAAQ,GAAI,KAAKC,KAAL,CAAWC,OAAX,CAAmBV,IAAnB,CAAlB;QACA,KAAKS,KAAL,CAAWE,MAAX,CAAkBH,SAAlB,EAA6B,CAA7B;QACAd,UAAU,CAAC,UAAD,EAAa,8BAAb,EAA6C,SAA7C,CAAV;MACD,CAbS,CAAV;IAcD,CAhBM;;IAiBPkB,YAAY,CAACZ,IAAD,EAAO;MACjB,MAAMQ,SAAQ,GAAI,KAAKC,KAAL,CAAWC,OAAX,CAAmBV,IAAnB,CAAlB;MACA,KAAKS,KAAL,CAAWD,SAAX,EAAsBK,IAAtB,GAA6B,IAA7B;MACAnB,UAAU,CAAC,UAAD,EAAa,kBAAb,EAAiC,SAAjC,CAAV;IACD;;EArBM;AALI,CAAf","names":["sweetalert","Todo","props","components","methods","deleteTodo","todo","title","text","type","showCancelButton","confirmButtonColor","confirmButtonText","closeOnConfirm","todoIndex","todos","indexOf","splice","completeTodo","done"],"sourceRoot":"","sources":["C:\\Users\\Егор\\vueproject\\praktika_zadanie2\\src\\components\\TodoList.vue"],"sourcesContent":["<template>\n  <div>\n    <p class=\"tasks\">Completed Tasks: {{todos.filter(todo => {return todo.done === true}).length}}</p>\n    <p class=\"tasks\">Pending Tasks: {{todos.filter(todo => {return todo.done === false}).length}}</p>\n    <todo v-on:delete-todo=\"deleteTodo\" v-on:complete-todo=\"completeTodo\" v-for=\"todo in todos\" :todo.sync=\"todo\"></todo>\n  </div>\n</template>\n\n<script type = \"text/javascript\" >\nimport sweetalert from 'sweetalert';\nimport Todo from './Todo';\n\nexport default {\n  props: ['todos'],\n  components: {\n    Todo,\n  },\n  methods: {\n    deleteTodo(todo) {\n      sweetalert({\n        title: 'Are you sure?',\n        text: 'This To-Do will be permanently deleted!',\n        type: 'warning',\n        showCancelButton: true,\n        confirmButtonColor: '#DD6B55',\n        confirmButtonText: 'Yes, delete it!',\n        closeOnConfirm: false,\n      },\n      () => {\n        const todoIndex = this.todos.indexOf(todo);\n        this.todos.splice(todoIndex, 1);\n        sweetalert('Deleted!', 'Your To-Do has been deleted.', 'success');\n      });\n    },\n    completeTodo(todo) {\n      const todoIndex = this.todos.indexOf(todo);\n      this.todos[todoIndex].done = true;\n      sweetalert('Success!', 'To-Do completed!', 'success');\n    },\n  },\n};\n</script>\n\n<style scoped>\np.tasks {\n  text-align: center;\n}\n</style>\n\n"]},"metadata":{},"sourceType":"module"}